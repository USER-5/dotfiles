{
	"keyboard.dispatch": "keyCode",
	"vim.cursorStylePerMode.insert": "block-outline",
	"vim.cursorStylePerMode.normal": "block",
	"vim.cursorStylePerMode.replace": "block-outline",
	"vim.cursorStylePerMode.visual": "block",
	"vim.cursorStylePerMode.visualblock": "block",
	"vim.cursorStylePerMode.visualline": "block",
	"vim.smartRelativeLine": true,
	"workbench.colorCustomizations": {
		"statusBar.background": "#005f5f",
		"statusBar.noFolderBackground": "#005f5f",
		"statusBar.debuggingBackground": "#005f5f"
	},
	"vim.statusBarColorControl": true,
	"vim.vimrc.path": "$HOME/.vim/vimrc",
	"vim.vimrc.enable": true,
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"editor.wordWrap": "on",
	"editor.wrappingIndent": "indent",
	"editor.rulers": [80],
	"editor.renderWhitespace": "boundary",
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"redhat.telemetry.enabled": true,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"java.project.importOnFirstTimeStartup": "automatic",
	"terminal.external.linuxExec": "bash",
	"prettier.useTabs": true,
	"editor.tabSize": 2,
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"git.confirmSync": false,
	"window.menuBarVisibility": "compact",
	"explorer.confirmDelete": false,
	"rust.wait_to_build": 500,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.proseWrap": "always",
	"prettier.quoteProps": "consistent",
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.formatOnSave": true
}
